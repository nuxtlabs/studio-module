{
  "name": "@nuxthq/studio",
  "version": "1.0.2",
  "homepage": "https://github.com/nuxtlabs/studio.nuxt.com/tree/dev/modules/studio",
  "repository": "https://github.com/nuxtlabs/studio.nuxt.com",
  "packageManager": "pnpm@8.9.2",
  "keywords": [
    "nuxt",
    "studio"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "theme.d.ts"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "types": "./dist/module.d.ts"
    },
    "./theme": {
      "import": "./dist/theme.mjs",
      "require": "./dist/theme.cjs",
      "types": "./theme.d.ts"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "scripts": {
    "prepare": "nuxt prepare playground",
    "dev": "NUXT_STUDIO_DEV_PROXY=true nuxt dev playground",
    "build": "nuxt-module-build",
    "lint": "eslint --ext .vue,.ts,.js,.mjs .",
    "prepack": "pnpm lint && pnpm build",
    "release": "pnpm lint && release-it",
    "pre:release": "pnpm lint && release-it --preRelease"
  },
  "dependencies": {
    "@nuxt/kit": "^3.8.0",
    "defu": "^6.1.3",
    "nuxt-component-meta": "^0.5.4",
    "nuxt-config-schema": "^0.4.6",
    "socket.io-client": "^4.7.2",
    "ufo": "^1.3.1"
  },
  "devDependencies": {
    "@nuxt-themes/alpine": "^1.6.4",
    "@nuxt-themes/elements": "0.9.5",
    "@nuxt/content": "^2.8.5",
    "@nuxt/module-builder": "^0.5.2",
    "@nuxt/schema": "^3.8.0",
    "@nuxtjs/color-mode": "^3.3.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/node": "^20.8.9",
    "eslint": "^8.52.0",
    "jiti": "^1.20.0",
    "nuxt": "^3.8.0",
    "pinceau": "^0.20.1",
    "release-it": "^16.2.1",
    "typescript": "^5.2.2"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "vue": "*"
      },
      "ignoreMissing": [
        "prosemirror-*",
        "@babel/core",
        "webpack",
        "axios",
        "postcss",
        "vue"
      ]
    }
  },
  "unbuild": {
    "entries": [
      "./src/module",
      "./src/theme"
    ],
    "externals": [
      "untyped"
    ],
    "rollup": {
      "emitCJS": true
    }
  }
}
