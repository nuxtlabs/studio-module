{
  "name": "@nuxthq/studio",
  "description": "Nuxt Studio is a visual editor for content-driven Nuxt application with Markdown.",
  "version": "1.0.6",
  "repository": "nuxtlabs/studio-module",
  "homepage": "https://nuxt.studio",
  "packageManager": "pnpm@8.10.0",
  "keywords": [
    "nuxt",
    "studio",
    "nuxtcms",
    "nuxtcontent"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "theme.d.ts"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "types": "./dist/module.d.ts"
    },
    "./theme": {
      "import": "./dist/theme.mjs",
      "require": "./dist/theme.cjs",
      "types": "./theme.d.ts"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "scripts": {
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground",
    "dev": "nuxt dev playground --tunnel",
    "build": "nuxt-module-build build",
    "lint": "eslint .",
    "prepack": "pnpm lint && pnpm build",
    "release": "pnpm lint && release-it",
    "pre:release": "pnpm lint && release-it --preRelease"
  },
  "dependencies": {
    "@nuxt/kit": "^3.9.0",
    "defu": "^6.1.3",
    "nuxt-component-meta": "^0.6.0",
    "socket.io-client": "^4.7.3",
    "ufo": "^1.3.2"
  },
  "devDependencies": {
    "@nuxt/content": "^2.10.0",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/image": "^1.1.0",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.9.0",
    "@nuxt/ui-pro": "^0.6.1",
    "@types/node": "^20.10.6",
    "consola": "^3.2.3",
    "eslint": "^8.56.0",
    "nuxt": "^3.9.0",
    "release-it": "^17.0.1",
    "typescript": "^5.3.3"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "vue": "*"
      },
      "ignoreMissing": [
        "@babel/core",
        "webpack",
        "axios",
        "postcss",
        "vue"
      ]
    }
  },
  "unbuild": {
    "entries": [
      "./src/module",
      "./src/theme"
    ],
    "externals": [
      "untyped"
    ],
    "rollup": {
      "emitCJS": true
    }
  }
}
