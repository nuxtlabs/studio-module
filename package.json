{
  "name": "@nuxthq/studio",
  "description": "Nuxt Studio is a visual editor for content-driven Nuxt application with Markdown.",
  "version": "2.2.1",
  "packageManager": "pnpm@9.15.6",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxtlabs/studio-module.git"
  },
  "homepage": "https://nuxt.studio",
  "keywords": [
    "vue",
    "nuxt",
    "markdown",
    "studio",
    "nuxtcms",
    "nuxtcontent"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "theme.d.ts"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "types": "./dist/module.d.ts"
    },
    "./theme": {
      "import": "./dist/theme.mjs",
      "require": "./dist/theme.cjs",
      "types": "./theme.d.ts"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "scripts": {
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground",
    "dev": "nuxt dev playground --tunnel",
    "build": "nuxt-module-build build",
    "typecheck": "nuxi typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepack": "pnpm lint && pnpm build",
    "release": "pnpm lint && release-it",
    "pre:release": "pnpm lint && release-it --preRelease"
  },
  "dependencies": {
    "@nuxt/kit": "^3.15.4",
    "defu": "^6.1.4",
    "git-url-parse": "^15.0.0",
    "nuxt-component-meta": "^0.10.0",
    "parse-git-config": "^3.0.0",
    "pkg-types": "^1.3.1",
    "socket.io-client": "^4.8.1",
    "ufo": "^1.5.4",
    "untyped": "^1.5.2"
  },
  "devDependencies": {
    "@nuxt/content": "^2.13.4",
    "@nuxt/eslint": "^0.7.6",
    "@nuxt/image": "^1.9.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.4",
    "@nuxt/ui-pro": "^1.7.0",
    "@types/node": "^22.13.9",
    "changelogen": "^0.6.1",
    "consola": "^3.4.0",
    "eslint": "^9.21.0",
    "nuxt": "^3.15.4",
    "release-it": "^17.11.0",
    "typescript": "^5.8.2"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "vue": "*"
      },
      "ignoreMissing": [
        "@babel/core",
        "webpack",
        "axios",
        "postcss",
        "vue"
      ]
    }
  },
  "unbuild": {
    "entries": [
      "./src/module",
      "./src/theme"
    ],
    "externals": [
      "untyped"
    ],
    "rollup": {
      "emitCJS": true
    }
  }
}
