{
  "name": "@nuxthq/studio",
  "version": "0.4.3",
  "homepage": "https://github.com/nuxtlabs/studio.nuxt.com/tree/dev/modules/studio",
  "repository": "https://github.com/nuxtlabs/studio.nuxt.com",
  "packageManager": "pnpm@7.20.0",
  "keywords": [
    "nuxt",
    "studio"
  ],
  "license": "UNLICENSED",
  "files": [
    "ui",
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "scripts": {
    "prepare": "nuxi prepare ui",
    "dev": "echo 'Press F5 in VS Code instead'",
    "play": "NUXT_STUDIO_DEV_PROXY=true nuxi dev playground",
    "ui": "PORT=3100 nuxi dev ui",
    "build": "yarn prepare && yarn build:module && yarn build:ui",
    "build:ui": "NITRO_PRESET=node nuxi build ui",
    "build:module": "nuxt-module-build",
    "lint": "eslint --ext .vue,.ts,.js,.mjs .",
    "prepublish": "yarn build",
    "prepack": "yarn build",
    "release": "yarn lint && release-it"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0",
    "@nuxt/schema": "^3.0.0",
    "defu": "^6.1.1",
    "nuxt-component-meta": "^0.4.2",
    "nuxt-config-schema": "^0.4.0",
    "socket.io-client": "^4.5.4",
    "ufo": "^1.0.1"
  },
  "devDependencies": {
    "@milkdown/core": "^6.5.4",
    "@milkdown/ctx": "^6.5.4",
    "@milkdown/plugin-collaborative": "^6.5.4",
    "@milkdown/plugin-cursor": "^6.5.4",
    "@milkdown/plugin-emoji": "^6.5.4",
    "@milkdown/plugin-history": "^6.5.4",
    "@milkdown/plugin-listener": "^6.5.4",
    "@milkdown/plugin-math": "^6.5.4",
    "@milkdown/plugin-slash": "^6.5.4",
    "@milkdown/plugin-tooltip": "^6.5.4",
    "@milkdown/plugin-trailing": "^6.5.4",
    "@milkdown/preset-commonmark": "^6.5.4",
    "@milkdown/preset-gfm": "^6.5.4",
    "@milkdown/prose": "^6.5.4",
    "@milkdown/theme-pack-helper": "^6.5.4",
    "@milkdown/utils": "^6.5.4",
    "@milkdown/vue": "^6.5.4",
    "@nuxt-themes/elements-edge": "^0.0.1-fd5890e",
    "@nuxt/content": "^2.3.0",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxthq/ui": "npm:@nuxthq/ui-edge@latest",
    "@nuxtjs/color-mode": "^3.2.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@vueuse/core": "^9.10.0",
    "@vueuse/nuxt": "^9.10.0",
    "eslint": "^8.31.0",
    "jiti": "^1.16.1",
    "monaco-editor-core": "^0.34.1",
    "nuxt": "^3.0.0",
    "pinceau": "^0.10.3",
    "release-it": "^15.6.0",
    "remark-mdc": "^1.1.3",
    "shiki-es": "^0.1.2",
    "splitpanes": "^3.1.5",
    "typescript": "^4.9.4",
    "y-websocket": "^1.4.5",
    "yjs": "^13.5.44"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "vue": "^3.2.0"
      },
      "ignoreMissing": [
        "prosemirror-*",
        "@babel/core",
        "webpack",
        "axios",
        "postcss"
      ]
    }
  }
}
