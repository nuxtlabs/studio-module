{
  "name": "@nuxthq/studio",
  "description": "Nuxt Studio is a visual editor for content-driven Nuxt application with Markdown.",
  "version": "1.0.15",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxtlabs/studio-module.git"
  },
  "homepage": "https://nuxt.studio",
  "keywords": [
    "vue",
    "nuxt",
    "markdown",
    "studio",
    "nuxtcms",
    "nuxtcontent"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "theme.d.ts"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "types": "./dist/module.d.ts"
    },
    "./theme": {
      "import": "./dist/theme.mjs",
      "require": "./dist/theme.cjs",
      "types": "./theme.d.ts"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "scripts": {
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground",
    "dev": "nuxt dev playground --tunnel",
    "build": "nuxt-module-build build",
    "typecheck": "nuxi typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepack": "pnpm lint && pnpm build",
    "release": "pnpm lint && release-it",
    "pre:release": "pnpm lint && release-it --preRelease"
  },
  "dependencies": {
    "@nuxt/kit": "^3.11.2",
    "defu": "^6.1.4",
    "git-url-parse": "^14.0.0",
    "nuxt-component-meta": "^0.6.4",
    "parse-git-config": "^3.0.0",
    "pkg-types": "^1.1.1",
    "socket.io-client": "^4.7.5",
    "ufo": "^1.5.3",
    "untyped": "^1.4.2"
  },
  "devDependencies": {
    "@nuxt/content": "^2.12.1",
    "@nuxt/eslint": "^0.3.12",
    "@nuxt/image": "^1.7.0",
    "@nuxt/module-builder": "^0.6.0",
    "@nuxt/schema": "^3.11.2",
    "@nuxt/ui-pro": "^1.2.0",
    "@types/node": "^20.12.12",
    "changelogen": "^0.5.5",
    "consola": "^3.2.3",
    "eslint": "^9.2.0",
    "nuxt": "^3.11.2",
    "release-it": "^17.2.1",
    "typescript": "^5.4.5"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "vue": "*"
      },
      "ignoreMissing": [
        "@babel/core",
        "webpack",
        "axios",
        "postcss",
        "vue"
      ]
    }
  },
  "unbuild": {
    "entries": [
      "./src/module",
      "./src/theme"
    ],
    "externals": [
      "untyped"
    ],
    "rollup": {
      "emitCJS": true
    }
  }
}
