{
  "name": "@nuxthq/studio",
  "version": "0.8.5",
  "homepage": "https://github.com/nuxtlabs/studio.nuxt.com/tree/dev/modules/studio",
  "repository": "https://github.com/nuxtlabs/studio.nuxt.com",
  "packageManager": "pnpm@7.20.0",
  "keywords": [
    "nuxt",
    "studio"
  ],
  "license": "UNLICENSED",
  "files": [
    "ui",
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./theme": {
      "import": "./dist/theme.mjs",
      "require": "./dist/theme.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "scripts": {
    "prepare": "nuxt prepare playground",
    "dev": "NUXT_STUDIO_DEV_PROXY=true nuxt dev playground",
    "build": "nuxt-module-build",
    "lint": "eslint --ext .vue,.ts,.js,.mjs .",
    "prepack": "yarn lint && yarn build",
    "release": "yarn lint && release-it"
  },
  "dependencies": {
    "@nuxt/kit": "^3.3.1",
    "defu": "^6.1.2",
    "nuxt-component-meta": "^0.5.1",
    "nuxt-config-schema": "^0.4.5",
    "socket.io-client": "^4.6.1",
    "ufo": "^1.1.1"
  },
  "devDependencies": {
    "@nuxt-themes/elements": "0.9.3",
    "@nuxt/content": "^2.5.2",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.3.1",
    "@nuxtjs/color-mode": "^3.2.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.36.0",
    "jiti": "^1.18.2",
    "nuxt": "^3.3.1",
    "pinceau": "^0.18.8",
    "release-it": "^15.9.1",
    "typescript": "^5.0.2"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "vue": "^3.2.0"
      },
      "ignoreMissing": [
        "prosemirror-*",
        "@babel/core",
        "webpack",
        "axios",
        "postcss"
      ]
    }
  },
  "unbuild": {
    "entries": [
      "./src/module",
      "./src/theme"
    ],
    "externals": [
      "untyped"
    ],
    "rollup": {
      "emitCJS": true
    }
  }
}
